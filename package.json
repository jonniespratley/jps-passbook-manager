{
  "name": "jps-passbook-manager",
  "version": "0.0.2",
  "config": {
    "delimiter": "pass-manager"
  },
  "bin": {
    "jps-passbook-manager": "./lib/cli.js",
    "pass-manager": "./cli.js"
  },
  "scripts": {
    "build": "grunt build -v",
    "clean": "rimraf .tmp data test/temp",
    "cli": "node cli.js",
    "coverage": "cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js",
    "debug-grunt": "iron-node node_modules/grunt-cli/bin/grunt build",
    "debug-gulp": "iron-node node_modules/gulp/bin/gulp.js",
    "debug-mocha": "iron-node node_modules/mocha/bin/_mocha",
    "istanbul": "istanbul cover ./node_modules/mocha/bin/_mocha --report lcovonly -- -R spec",
    "mocha": "mocha test/specs",
    "pretest": "rimraf test/temp",
    "start": "node server.js",
    "test": "gulp test",
    "watch": "export DEBUG=jps-passbook-manager:*; nodemon -w routes/ -w lib/ server.js"
  },
