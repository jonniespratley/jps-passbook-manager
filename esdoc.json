{
  "title": "Passbook Manager",
  "source": "./lib",
  "destination": "./esdoc",

  "excludes": [
    "-spec\\.(js|es6)$",
    "\\.config\\.(js|es6)$"
  ],

  "autoPrivate": true,
  "unexportIdentifier": false,
  "undocumentIdentifier": true,
  "builtinExternal": true,
  "index": "./README.md",
  "package": "./package.json",
  "coverage": true,
  "includeSource": true,
  "lint": true,
  "test": {
    "type": "mocha",
    "source": "./test/specs"
  },
  "manual": {
    "asset": "./manual/asset",
    "overview": [
      "./manual/overview.md"
    ],
    "installation": [
      "./manual/installation.md"
    ],
    "usage": [
      "./manual/usage.md"
    ],
    "tutorial": [
      "./manual/tutorial.md"
    ],
    "configuration": [
      "./manual/configuration.md"
    ],
    "example": [
      "./manual/example.md"
    ],
    "faq": [
      "./manual/faq.md"
    ],
    "changelog": [
      "./CHANGELOG.md"
    ]
  }
}
